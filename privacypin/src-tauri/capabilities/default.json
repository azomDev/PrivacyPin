{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": [
		"main"
	],
	"permissions": [
		"core:default",
		{
		"identifier": "opener:default",
		"allow": [
			{
				"open": true
			}
		]
		},
		"store:default",
		{
			"identifier": "http:default",
			"allow": [
			{
				"url": "http://**"
			},
			{
				"url": "https://**"
			},
			{
				"url": "http://*:*"
			},
			{
				"url": "https://*:*"
			}
			]
		},
		"http:default"
	]
}
